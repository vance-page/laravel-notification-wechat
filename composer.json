{
    "name": "vance-page/laravel-notification-wechat",
    "description": "Laravel Wechat Notifications Driver",
    "keywords": ["laravel", "wechat", "notification", "template"],
    "support": {
        "issues": "https://github.com/vance-page/laravel-notification-wechat/issues",
        "source": "https://github.com/vance-page/laravel-notification-wechat"
    },
    "authors": [
        {
            "name": "vance",
            "homepage": "https://github.com/vance-page"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^6.0 || ^7.0 || ^7.0 || ^9.0",
        "illuminate/support": "^6.0 || ^7.0 || ^7.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Vance\\LaravelNotificationWechat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vance\\LaravelNotificationWechat\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vance\\LaravelNotificationWechat\\WechatServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
